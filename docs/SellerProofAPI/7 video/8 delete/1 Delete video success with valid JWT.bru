meta {
  name: Delete video success with valid JWT
  type: http
  seq: 1
}

delete {
  url: {{baseUrl}}/api/v1/video/{{testVideoId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{adminAccessToken}}
}

tests {
  test('Should return 200 OK and confirm deletion for valid JWT token', function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.be.an('object');
    expect(res.body.message).to.be.a('string');
    expect(res.body.message.toLowerCase()).to.include('video deleted');

    // Save for restore tests
    bru.setEnvVar("deletedVideoId", bru.getEnvVar("testVideoId"));
  });
}
