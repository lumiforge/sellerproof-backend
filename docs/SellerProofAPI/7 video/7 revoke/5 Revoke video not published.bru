meta {
  name: Revoke video not published
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/api/v1/video/revoke
  body: json
  auth: bearer
}

auth:bearer {
  token: {{adminAccessToken}}
}

body:json {
  {
    "video_id": "{{testSecVideoId}}"
  }
}

tests {
  test("Should return 400 Bad Request", function() {
    expect(res.status).to.equal(400);
  });

  test("Should return error message", function() {
    expect(res.body.message).to.include("video is not published");
  });
}