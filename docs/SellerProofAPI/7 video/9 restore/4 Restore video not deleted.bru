meta {
  name: Restore video not deleted
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/api/v1/video/restore
  body: json
  auth: bearer
}

auth:bearer {
  token: {{adminAccessToken}}
}

body:json {
  {
    "video_id": "{{deletedVideoId}}"
  }
}

tests {
  test('Should return 404 because video is not in trash', function() {
    expect(res.status).to.equal(404);
    expect(res.body.message).to.include('not found');
  });
}