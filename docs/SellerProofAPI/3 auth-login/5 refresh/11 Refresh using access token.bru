meta {
  name: Refresh using access token
  type: http
  seq: 11
}

post {
  url: {{baseUrl}}/api/v1/auth/refresh
  body: json
  auth: inherit
}

body:json {
  {
    "refresh_token": "{{adminAccessToken}}"
  }
}

tests {
  test('Should return 401 Unauthorized when using access token instead of refresh token', function() {
    expect(res.status).to.equal(401);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Unauthorized');
  });
}