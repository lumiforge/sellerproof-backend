meta {
  name: Verify Email incorrect
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/auth/verify-email
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{adminEmail}}",
    "code": "000000"
  }
}

tests {
  test('Should return 500 Internal Server Error for invalid verification code', function() {
    expect(res.status).to.equal(500);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Internal Server Error');
  });
}