meta {
  name: Invite user with expired JWT token
  type: http
  seq: 10
}

post {
  url: {{baseUrl}}/api/v1/organization/invite
  body: json
  auth: bearer
}

auth:bearer {
  token: {{expiredAccessToken}}
}

body:json {
  {
    "email": "test@example.com",
    "role": "user",
    "org_id": "{{adminOrgId}}"
  }
}

tests {
  test('Should return 401 Unauthorized for request with expired JWT token', function() {
    expect(res.status).to.equal(401);
  });
}