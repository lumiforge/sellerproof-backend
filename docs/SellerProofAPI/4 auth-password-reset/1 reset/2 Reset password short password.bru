meta {
  name: Reset password short password
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/auth/reset-password
  body: json
  auth: none
}

body:json {
  {
    "email": "{{adminEmail}}",
    "code": "123456",
    "new_password": "123"
  }
}

tests {
  test('Should return 400 Bad Request for short password', function() {
    expect(res.status).to.equal(400);
    expect(res.body.message).to.include('password must be');
  });
}