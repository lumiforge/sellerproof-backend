    "definitions": {
        "http.CompleteMultipartUploadRequest": {
            "description": "Multipart upload completion request",
            "type": "object",
            "required": [
                "parts",
                "video_id"
            ],
            "properties": {
                "parts": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/http.CompletedPart"
                    }
                },
                "video_id": {
                    "type": "string"
                }
            }
        },
        "http.CompleteMultipartUploadResponse": {
            "description": "Multipart upload completion response",
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "video_url": {
                    "type": "string"
                }
            }
        },
        "http.CompletedPart": {
            "description": "Completed multipart upload part",
            "type": "object",
            "properties": {
                "etag": {
                    "type": "string"
                },
                "part_number": {
                    "type": "integer"
                }
            }
        },
        "http.CreateShareLinkRequest": {
            "description": "Create share link request",
            "type": "object",
            "required": [
                "video_id"
            ],
            "properties": {
                "expires_in_hours": {
                    "type": "integer",
                    "minimum": 0
                },
                "video_id": {
                    "type": "string"
                }
            }
        },
        "http.CreateShareLinkResponse": {
            "description": "Create share link response",
            "type": "object",
            "properties": {
                "expires_at": {
                    "type": "integer"
                },
                "share_url": {
                    "type": "string"
                }
            }
        },
        "http.ErrorResponse": {
            "description": "Error response with details",
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "error": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "http.GetPartUploadURLsRequest": {
            "description": "Part upload URLs request",
            "type": "object",
            "required": [
                "total_parts",
                "video_id"
            ],
            "properties": {
                "total_parts": {
                    "type": "integer",
                    "minimum": 1
                },
                "video_id": {
                    "type": "string"
                }
            }
        },
        "http.GetPartUploadURLsResponse": {
            "description": "Part upload URLs response",
            "type": "object",
            "properties": {
                "expires_at": {
                    "type": "integer"
                },
                "part_urls": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "http.GetPublicVideoResponse": {
            "description": "Get public video response",
            "type": "object",
            "properties": {
                "download_url": {
                    "type": "string"
                },
                "expires_at": {
                    "type": "integer"
                },
                "file_name": {
                    "type": "string"
                },
                "file_size": {
                    "type": "integer"
                }
            }
        },
        "http.HealthResponse": {
            "description": "Health check response",
            "type": "object",
            "properties": {
                "status": {
                    "type": "string"
                },
                "timestamp": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "http.InitiateMultipartUploadRequest": {
            "description": "Multipart upload initiation request",
            "type": "object",
            "required": [
                "file_name",
                "file_size_bytes"
            ],
            "properties": {
                "duration_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "file_name": {
                    "type": "string"
                },
                "file_size_bytes": {
                    "type": "integer",
                    "minimum": 1
                }
            }
        },
        "http.InitiateMultipartUploadResponse": {
            "description": "Multipart upload initiation response",
            "type": "object",
            "properties": {
                "recommended_part_size_mb": {
                    "type": "integer"
                },
                "upload_id": {
                    "type": "string"
                },
                "video_id": {
                    "type": "string"
                }
            }
        },
        "http.LoginRequest": {
            "description": "Login request with email and password",
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "http.LoginResponse": {
            "description": "Login response with tokens and user info",
            "type": "object",
            "properties": {
                "access_token": {
                    "type": "string"
                },
                "expires_at": {
                    "type": "integer"
                },
                "refresh_token": {
                    "type": "string"
                },
                "user": {
                    "$ref": "#/definitions/http.UserInfo"
                }
            }
        },
        "http.LogoutRequest": {
            "description": "Logout request with refresh token",
            "type": "object",
            "required": [
                "refresh_token"
            ],
            "properties": {
                "refresh_token": {
                    "type": "string"
                }
            }
        },
