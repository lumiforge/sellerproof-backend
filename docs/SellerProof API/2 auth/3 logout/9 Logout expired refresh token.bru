meta {
  name: Logout expired refresh token
  type: http
  seq: 9
}

post {
  url: {{baseUrl}}/api/v1/auth/logout
  body: json
}

headers {
  Authorization: Bearer {{adminAccessToken}}
}

body:json {
  {
    "refresh_token": "{{expiredRefreshToken}}"
  }
}

tests {
  test('Should return 401 for expired refresh token', function() {
    expect(res.status).to.equal(401);
  });
}