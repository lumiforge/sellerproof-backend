meta {
  name: Switch organization without JWT token
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/auth/switch-organization
  body: json
  auth: inherit
}

body:json {
  {
    "org_id": "{{adminOrgId}}"
  }
}

tests {
  test('Should return 401 Unauthorized for switch organization request without JWT token', function() {
    expect(res.status).to.equal(401);
  });
}