meta {
  name: Get audit logs with server error simulation
  type: http
  seq: 7
}

get {
  url: {{baseUrl}}/api/v1/admin/audit-logs?action_type=force_error
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

tests {
  test('Should return 500 Internal Server Error when backend fails', function() {
    expect(res.status).to.equal(500);
    expect(res.body).to.be.an('object');
    expect(res.body).to.have.property('error');
  });
}
