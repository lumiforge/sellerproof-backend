meta {
  name: Get audit logs with invalid limit parameter
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/v1/admin/audit-logs?limit=5000
  auth: bearer
}

auth:bearer {
  token: {{adminAccessToken}}
}

tests {
  test('Should clamp invalid limit to maximum allowed value and still return 200 OK', function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.be.an('object');
    expect(res.body.limit).to.be.at.most(1000);
  });
}
