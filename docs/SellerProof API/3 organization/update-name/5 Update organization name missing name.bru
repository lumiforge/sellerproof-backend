meta {
  name: Update organization name missing name
  type: http
  seq: 5
}

put {
  url: {{baseUrl}}/api/v1/organization/name?org_id={{adminOrgId}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{adminAccessToken}}
}

body:json {
  {
    "name": ""
  }
}

tests {
  test('Should return 400 Bad Request', function() {
    expect(res.status).to.equal(400);
    expect(res.body.message).to.include('name');
    expect(res.body.message).to.include('required');
  });
}