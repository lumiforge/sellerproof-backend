meta {
  name: Setup - Simple user accepts invitation
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/api/v1/organization/invitations/accept
  body: json
  auth: bearer
}

auth:bearer {
  token: {{simpleUserAccessToken}}
}

body:json {
  {
    "invite_code": "{{userInviteCode}}"
  }
}

tests {
  test('Should accept user invitation', function() {
    expect(res.status).to.equal(200);
    expect(res.body.role).to.equal('user');
  });
}