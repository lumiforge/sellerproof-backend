meta {
  name: Delete invitation with invalid JWT token
  type: http
  seq: 3
}

delete {
  url: {{baseUrl}}/api/v1/organization/invitations/{{validInvitationId}}
  auth: bearer
}

auth:bearer {
  token: {{invalidAccessToken}}
}

tests {
  test('Should return 401 Unauthorized when invalid JWT token is provided', function() {
    expect(res.status).to.equal(401);
    expect(res.body).to.be.an('object');
    
    // Validate error response structure
    expect(res.body).to.have.property('error');
    expect(res.body).to.have.property('message');
    
    // Validate field types
    expect(res.body.error).to.be.a('string');
    expect(res.body.message).to.be.a('string');
    
    // Validate field values are not empty
    expect(res.body.error).to.not.equal('');
    expect(res.body.message).to.not.equal('');
    
    // Validate error message content
    expect(res.body.error.toLowerCase()).to.include('unauthorized').or.include('invalid').or.include('token');
  });
}