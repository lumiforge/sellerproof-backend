meta {
  name: Register email already exists
  type: http
  seq: 13
}

post {
  url: {{baseUrl}}/api/v1/auth/register
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{testEmail}}",
    "password": "password123",
    "full_name": "Test User"
  }
}

tests {
  test('Should return 409 for email already exists', function() {
    expect(res.status).to.equal(409);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Conflict');
  });
}