meta {
  name: Refresh token empty string
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/api/v1/auth/refresh
  body: json
  auth: inherit
}

body:json {
  {
    "refreshToken": ""
  }
}

tests {
  test('Should return 400 Bad Request for empty refreshToken string', function() {
    expect(res.status).to.equal(400);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Bad Request');
  });
}