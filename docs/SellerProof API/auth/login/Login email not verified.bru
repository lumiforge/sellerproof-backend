meta {
  name: Login email not verified
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/api/v1/auth/login
  body: json
  auth: none
}

body:json {
  "email": "{{testEmail}}",
  "password": "{{testEmailPassword}}"
}

tests {
  test("Should return 403 Forbidden for unverified email", function() {
    expect(res.status).to.equal(403);
    expect(res.body.error).to.equal("Forbidden");
    expect(res.body.message).to.equal("email not verified");
  });
}