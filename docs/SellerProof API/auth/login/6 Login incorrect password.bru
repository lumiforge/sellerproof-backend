meta {
  name: Login incorrect password
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/api/v1/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{testEmail}}",
    "password": "wrongpassword"
  }
}

tests {
  test('Should return 400 Bad Request for incorrect password', function() {
    expect(res.status).to.equal(400);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Bad Request');
  });
}