meta {
  name: Complete upload success with valid JWT
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/video/upload/complete
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "video_id": "{{testVideoId}}",
    "parts": [
      {
        "part_number": 1,
        "etag": "{{headerEtag1}}"
      },
      {
        "part_number": 2,
        "etag": "{{headerEtag2}}"
      },
      {
        "part_number": 3,
        "etag": "{{headerEtag3}}"
      }
    ]
  }
}

tests {
  test('Should return 200 OK and valid upload completion response for valid JWT token', function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.be.an('object');
    
    // Validate all required fields exist
    expect(res.body).to.have.property('message');
    expect(res.body).to.have.property('video_url');
    
    // Validate field types
    expect(res.body.message).to.be.a('string');
    expect(res.body.video_url).to.be.a('string');
    
    // Validate field values are not empty
    expect(res.body.message).to.not.equal('');
    expect(res.body.video_url).to.not.equal('');
    
    // Validate message content
    expect(res.body.message).to.include('completed');
  });
}