meta {
  name: Revoke video success with manager JWT
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/video/revoke
  body: json({
    "video_id": "test-video-id-456"
  })
  headers: {
    Content-Type: application/json
    Authorization: Bearer {{validManagerJWT}}
  }
}

assert {
  res.status: 200
  res.body.message: contains("Video access revoked successfully")
  res.body.video_id: equals("test-video-id-456")
  res.body.status: equals("private")
}