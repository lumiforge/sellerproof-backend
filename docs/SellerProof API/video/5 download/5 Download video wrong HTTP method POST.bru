meta {
  name: Download video wrong HTTP method POST
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/api/v1/video/download?video_id=sample-video-id
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

tests {
  test('Should return 405 Method Not Allowed when using POST method', function() {
    expect(res.status).to.equal(405);
    expect(res.body).to.be.an('object');
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Method not allowed');
  });
}