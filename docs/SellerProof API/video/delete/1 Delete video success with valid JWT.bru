meta {
  name: Delete video success with valid JWT
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/video/delete
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "video_id": "{{testVideoId}}"
  }
}

tests {
  test('Should return 200 OK and confirm deletion for valid JWT token', function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.be.an('object');

    expect(res.body).to.have.property('success');
    expect(res.body.success).to.be.a('boolean');
    expect(res.body.success).to.equal(true);

    if (res.body.message) {
      expect(res.body.message).to.be.a('string');
      expect(res.body.message.toLowerCase()).to.include('delete');
    }
  });
}
