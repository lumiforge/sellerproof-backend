meta {
  name: Search videos with malformed JWT token
  type: http
  seq: 8
}

get {
  url: {{baseUrl}}/api/v1/video/search
  auth: bearer
}

auth:bearer {
  token: not.a.jwt.token.at.all
}

tests {
  test('Should return 401 Unauthorized for malformed JWT token', function() {
    expect(res.status).to.equal(401);
    expect(res.body).to.have.property('error');
    expect(res.body.error).to.include('Unauthorized');
  });
}