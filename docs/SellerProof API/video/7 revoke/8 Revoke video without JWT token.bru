meta {
  name: Revoke video without JWT token
  type: http
  seq: 8
}

post {
  url: {{baseUrl}}/api/v1/video/revoke
  body: json({
    "video_id": "test-video-id-123"
  })
  headers: {
    Content-Type: application/json
  }
}

assert {
  res.status: 401
  res.body.error: contains("Unauthorized")
  res.body.message: contains("User not authenticated")
}