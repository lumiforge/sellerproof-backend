meta {
  name: Revoke video without JWT token
  type: http
  seq: 8
}

post {
  url: {{baseUrl}}/api/v1/video/revoke
  body: json
  auth: none
}

body:json {
  {
    "video_id": "test-video-id-123"
  }
}

tests {
  test("Should return 401 Unauthorized", function() {
    expect(res.status).to.equal(401);
  });

  test("Should return error message", function() {
    expect(res.body.error).to.include("Unauthorized");
  });
}